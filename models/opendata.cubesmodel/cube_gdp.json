{
    "name": "gdp",
    "label": "World Gross Domestic Product 1960-2011",
    "category": "Open Data",
    "info": {
        "min_date": "1960-01-01",
        "max_date": "2011-12-31"
    },
    "dimensions": [ "year", "country" ],

    "measures": [
        {
            "name": "value", 
            "label": "GDP in current USD",
            "info": { "measurement_type": "money" }
        }
    ],

    "aggregates": [
        {
            "name": "gdp_avg",
            "label": "Average GDP in current USD",
            "measure": "value",
            "function": "avg",
            "info": { "measurement_type": "money" }
        }
    ],

    "mappings": {
        "country.name": "country_name",
        "country.code": "country_code"
    }
}
